<h1>Welcome to the <strong><%= @list.name.upcase %></strong> list</h1>
<p><% @list.bookmarks.each do |bookmark| %></p>
  <p><%= bookmark.movie.title %></p>
  <p><%= bookmark.comment %></p>
<% end %>
<p><%= link_to "Create a bookmark", new_list_bookmark_path(@list) %></p>
<p><%= link_to "Go Back", lists_path %></p>
